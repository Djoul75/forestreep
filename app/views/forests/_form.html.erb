<%= simple_form_for(@forest) do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :address %>
  <%= f.input :tree %>
  <%= f.input :animals %>
  <%= f.input :price %>
  <%= f.input :size %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit %>
<% end %>
