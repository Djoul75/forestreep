<div class="container-review-new">
  <h1>Add a new review to <strong><%= @forest.title %></strong></h1>
  <%= simple_form_for [@forest, @review] do |f| %>
    <%= f.input :comment, placeholder: 'Your comment' %>
    <%= f.input :rating, collection: 0..5, placeholder: '1-5' %>
    <%= f.submit class: 'btn submit-button'%>
  <% end %>
</div>
