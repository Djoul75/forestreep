<h1>Add a reservation</h1>

<%= simple_form_for [@forest, @booking]  do |f| %>
  <%= f.input :start_date, as: :date, label: "Start date", placeholder: "From", wrapper_html: { class: "inline_field_wrapper" } %>
  <%= f.input :end_date, as: :date, label: "End date", placeholder: "To", wrapper_html: { class: "inline_field_wrapper" } %>
  <%= f.submit "Submit booking", class: "btn btn-primary" %>
<% end %>
